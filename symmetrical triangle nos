
public class Main
{
	public static void main(String[] args) {
	    for(int i=1;i<=5;i++){
	        int s;
	        for(s=1;s<=5-i;s++){
	            System.out.print(" ");
	        }
	        for(int j=i;j>=1;j--){
	            System.out.print(j);
	        }
	        for(int j=2;j<=i;j++){
	            System.out.print(j);
	        }
	        System.out.println(); 
	    }
		
	}
